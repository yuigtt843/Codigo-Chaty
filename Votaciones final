        eleccion_alcaldia= input("Hacia que partido va dirigido su voto? ")
        
        if eleccion_alcaldia!="C" and eleccion_alcaldia!="D" and eleccion_alcaldia!="E" and eleccion_alcaldia!="F":
            print("Su voto es invalido")
            eleccion_alcaldia="voto nulo"

        else:
            if eleccion_alcaldia== "C":
                print("Su voto fue hacia el partido VALOR")

            if eleccion_alcaldia=="D":
                print("Su voto fue hacia el partido UNE")

            if eleccion_alcaldia=="E":
                print("Su voto fue hacia el partido PAN")

            if eleccion_alcaldia=="F":
                print("Su voto fue hacia el partido VAMOS")   
    
            ultima_confirmacion= input("Desea confirmar sus votos? Chi?, Ño? ") 
            
            if ultima_confirmacion!="Chi"and eleccion_alcaldia!="Ño":
                print("No se ha confirmado nada, BABOSO")
